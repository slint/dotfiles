{
  "[css]": { "editor.tabSize": 2 },
  "[elixir]": { "editor.tabSize": 2 },
  "[graphql]": { "editor.tabSize": 2 },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[scss]": { "editor.tabSize": 2 },
  "[vue-html]": { "editor.tabSize": 2 },
  "[vue]": { "editor.tabSize": 2 },
  "breadcrumbs.enabled": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.diagnosticLevel": "Hint",
  "docker.showExplorer": false,
  "editor.codeActionsOnSave": {},
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontFamily": "'Hack Nerd Font Mono', 'Droid Sans Mono', 'Courier New', 'Droid Sans Fallback', monospace",
  "editor.fontSize": 12,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 90,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [79, 88],
  "editor.snippetSuggestions": "inline",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": true,
  "files.associations": {
    "*.env": "yaml",
    "*.html": "jinja-html"
  },
  "files.defaultLanguage": "python",
  "files.exclude": {
    "**/__pycache__": true,
    "**/_build": true,
    "**/.cache": true,
    "**/.coverage": true,
    "**/.coverage.*": true,
    "**/.DS_Store": true,
    "**/.elixir_ls": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.pytest_cache": true,
    "**/.svn": true,
    "**/.vscode/tags": true,
    "**/*.egg-info": true,
    "**/*.eggs": true,
    "**/*.pid": true,
    "**/*.pyc": true,
    "**/*.secret": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/CVS": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "fileutils.delete.useTrash": true,
  "fileutils.typeahead.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.codeLens.scopes": ["document", "containers"],
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "chorded",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "liveshare.featureSet": "insiders",
  "markdown.extension.toc.levels": "2..6",
  "python.formatting.blackPath": "${env:HOME}/.local/bin/black",
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "python.linting.pydocstyleEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.venvPath": "${env:HOME}/Envs",
  "python.workspaceSymbols.exclusionPatterns": [
    "**/site-packages/**",
    "**/site-packages/**/*.pyc"
  ],
  "search.exclude": { "**/.pytest_cache": true, "**/.vscode": true },
  "search.useGlobalIgnoreFiles": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "terminal.integrated.fontSize": 11,
  "window.menuBarVisibility": "toggle",
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "window.titleBarStyle": "native",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {},
  "workbench.colorTheme": "Material Eighties Night",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.keyboardNavigation": "filter",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "newUntitledFile",
  "vsicons.dontShowNewVersionMessage": true,
  "discord.enabled": false
}
